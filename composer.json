{
    "name": "sugiphp/assets",
    "type": "library",
    "description": "SugiPHP Assets Management Component based on Assetic",
    "keywords": ["assets", "css", "javascript", "less", "Assetic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Plamen Popov",
            "email": "tzappa@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "kriswallsmith/assetic": "1.1.*",
        "natxet/CssMin": "dev-master",
        "tedivm/jshrink": "0.5.2"
    },
    "require-dev": {
        "leafo/lessphp": "dev-master",
        "leafo/scssphp": "dev-master"
    },
    "suggests": {
        "leafo/lessphp": "Enables ability to work with less files",
        "leafo/scssphp": "Enables ability to work with SCSS files"
    },
    "autoload": {
        "psr-4": {
            "SugiPHP\\Assets\\": ""
        }
    },
    "minimum-stability": "dev"
}
